{
    "manifest_version": 2,
    "name": "GitHub Markdown Navigator",
    "version": "0.2",
    "description": "Helps navigating markdown files in GitHub by providing a table of contents.",
    "homepage_url": "https://github.com/timbussmann/GitHubMarkdownNavigator",
    "permissions": [],
    "icons": { 
        "16": "open-book16.png",
       "128": "open-book128.png" },
    "content_scripts": [
        {
          "matches": ["*://github.com/*"],
          "css": ["navCss.css"],
          "js": ["contentScript.js"]
        }
      ]
}